{
  "name": "@ibrains-design/file-utils",
  "version": "0.77.1",
  "description": "JS File API utilities",
  "keywords": [
    "js",
    "utils",
    "file-utils"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "publishConfig": {
    "access": "public"
  },
  "clean-package": "../../../clean-package.config.json",
  "dependencies": {
    "@ibrains-design/i18n-utils": "workspace:*"
  },
  "devDependencies": {
    "clean-package": "2.2.0"
  }
}
